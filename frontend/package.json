{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^7.2.1",
        "@types/jest": "^24.9.1",
        "@types/node": "^12.12.62",
        "@types/react": "^16.9.50",
        "@types/react-dom": "^16.9.8",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-hook-form": "^6.9.0",
        "react-scripts": "3.4.3",
        "superagent": "^6.1.0",
        "tailwindcss": "^1.8.12",
        "typescript": "^3.7.5",
        "unity-publisher-api": "0.1.0"
    },
    "scripts": {
        "start": "set PORT=3333 && tsc -b && react-scripts start",
        "prestart": "npm run build:tailwind",
        "build": "react-scripts build",
        "build:tailwind": "npx tailwind build src/styles/tailwind.css -o src/styles/tailwind.output.css -c tailwind.config.js",
        "prebuild": "cross-env NODE_ENV=production npm run build:tailwind",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "proxy": "http://localhost:3000",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/superagent": "^4.1.10",
        "cross-env": "^7.0.2",
        "module-alias": "^2.2.2",
        "node-sass": "^4.14.1"
    }
